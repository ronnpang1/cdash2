@model IEnumerable<Inspinia_MVC5.Models.committee_Memberlist>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>List of committee_Memberlist</h5>
					<div class="ibox-tools">
						@Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-primary btn-xs"})
					</div>
                </div>
                <div class="ibox-content">

<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name_ofCommittee)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Uname_ofMember)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.member_joindate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.memeber_endate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.active)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name_ofCommittee)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Uname_ofMember)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.member_joindate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.memeber_endate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.active)
        </td>
        <td>
			@Html.ActionLink("Details", "Details", new { id=item.ID }, new { @class = "btn btn-primary btn-sm"})
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }, new { @class = "btn btn-white btn-sm"})
            @Html.ActionLink("Delete", "Delete", new { id=item.ID }, new { @class = "btn btn-white btn-sm"})
        </td>
    </tr>
}

</table>

                </div>
            </div>
        </div>
    </div>
 </div>



